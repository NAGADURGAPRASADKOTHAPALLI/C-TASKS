
#include<stdio.h>
void main()
{
	int a[5],i,j,count;
	
	printf("enter the elements:");
	for(i=0;i<5;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("the prime numbers are:");
	for(i=0;i<5;i++)
	{
		count=0;
		for(j=2;j<a[i];j++)
		{
			if(a[i]%j==0)
			{
			count=1;
			break;
		    }
		}
		if(count==0)
	printf("%d\t",a[i]);
	}
	
	
}
